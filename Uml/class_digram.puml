@startuml ClassDiagram

class domain_routes {
  +get_domain_questions(domain_name: str)
}

class pre_elicitation_routes {
  +save_response(data)
}

class domain_service {
  +load_domain_knowledge(domain_name: str)
}

class elicitation_question_service {
  +generate_domain_questions(domain_knowledge: dict)
}

class pre_elicitation_storage_service {
  +store_response(domain: str, question: str, answer: str)
}

class PreElicitationResponse {
  id: int
  domain: str
  question: str
  answer: str
}

domain_routes --> domain_service
domain_routes --> elicitation_question_service

pre_elicitation_routes --> pre_elicitation_storage_service
pre_elicitation_storage_service --> PreElicitationResponse
@enduml
